<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/background">
        <shape android:shape="oval">
            <solid android:color="@color/has_finite_source" />
            <size
                android:width="@dimen/marker_icon_size_small"
                android:height="@dimen/marker_icon_size_small" />
        </shape>
    </item>
    <item android:id="@+id/foreground">
        <shape android:shape="oval">
            <stroke
                android:width="@dimen/icon_stroke_width"
                android:color="@color/black" />
        </shape>
    </item>
</layer-list>
